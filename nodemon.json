{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --debug --harmony"
  },
  "events": {
    "restart": "scripts/notify-nodemon.sh"
  },
  "env": {
    "NODE_ENV": "development",
    "DEBUG": "express:*"
  },
  "ext": "js jsx"
}
